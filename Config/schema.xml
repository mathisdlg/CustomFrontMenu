          xsi:noNamespaceSchemaLocation="../../../../vendor/thelia-modules/propel/resources/xsd/database.xsd" >

    <table name="custom_front_menu" namespace="CustomFrontMenu\Model">
        <column autoIncrement="true" name="id" primaryKey="true" required="true" type="INTEGER"/>
    </table>

    <table name="custom_front_menu_child" namespace="CustomFrontMenu\Model">
        <column autoIncrement="true" name="id" primaryKey="true" required="true" type="INTEGER"/>
        <column name="menu_id" required="true" type="INTEGER"/>
        <foreign-key foreignTable="custom_front_menu">
            <reference local="menu_id" foreign="id"/>
        </foreign-key>
    </table>

</database>
